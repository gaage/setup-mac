
#VI Editor aliases
alias gvim="mvim $1"

#Python homebrew
alias python2=/usr/local/opt/python@2/bin/python2

# Open macapps from commandline using 'open -a'
alias finder='open -a finder'
alias fluor='open -a Fluor'
alias preview='open -a Preview $@'

# cd aliases
alias ..="cd .."
alias ...="cd ../.."
alias sm='cd $HOME/.setup-mac'

# Additional user aliases (with confidential contens?)
# Like:
# alias matlab='/Applications/MATLAB_R2019a.app/bin/matlab -nodesktop -nosplash'
# alias dreamhost='ssh user@domain'
[ -e $HOME/.aliases ] && source $HOME/.aliases
